{
    "name": "emran/logstash-bundle",
    "type": "symfony-bundle",
    "description": "Symfony2 Bundle for writing logs to logstash through redis broker/input.",
    "keywords": ["log", "logging"],
    "homepage": "http://phpfour.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohammad Emran Hasan",
            "email": "phpfour@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/monolog-bundle": ">=2.1",
        "symfony/monolog-bridge": ">=2.1",
        "symfony/dependency-injection": ">=2.1",
        "symfony/config": ">=2.1",
        "monolog/monolog": "~1.3"
    },
    "require-dev": {
        "symfony/yaml": ">=2.1"
    },
    "autoload": {
        "psr-0": { "Emran\\Bundle\\LogstashBundle": "src/" }
    }
}
